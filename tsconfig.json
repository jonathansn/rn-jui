{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "jsx": "react-native",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": ["dom", "esnext"],
    "noEmit": true,
    "resolveJsonModule": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}